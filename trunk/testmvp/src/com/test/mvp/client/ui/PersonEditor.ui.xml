<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:e='urn:import:com.google.gwt.editor.ui.client'>
	<ui:style src="../common.css">
		
	</ui:style>
	<g:HTMLPanel>
		<div class="{style.rightAlign}">
			First Name:
			<e:ValueBoxEditorDecorator ui:field="firstName"
				stylePrimaryName="{style.editField}">
				<e:valuebox>
					<g:TextBox ui:field="firstNameBox" stylePrimaryName="{style.editField}" />
				</e:valuebox>
			</e:ValueBoxEditorDecorator>
			<br />
			Last Name:
			<e:ValueBoxEditorDecorator ui:field="lastName"
				stylePrimaryName="{style.editField}">
				<e:valuebox>
					<g:TextBox stylePrimaryName="{style.editField}" />
				</e:valuebox>
			</e:ValueBoxEditorDecorator>
			<br />
			Email Id:
			<e:ValueBoxEditorDecorator ui:field="emailId"
				stylePrimaryName="{style.editField}">
				<e:valuebox>
					<g:TextBox stylePrimaryName="{style.editField}" />
				</e:valuebox>
			</e:ValueBoxEditorDecorator>
			<br />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>